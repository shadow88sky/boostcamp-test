generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id          Int       @id @default(autoincrement())
  name        String?   @db.VarChar(255)
  address     String?   @db.VarChar(255)
  description String?
  createdAt   DateTime? @default(now()) @db.Timestamptz(6)
}
